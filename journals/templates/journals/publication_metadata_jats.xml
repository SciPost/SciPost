<article dtd-version="1.1d3">
<front>
<journal-meta>
<publisher>
<publisher-name>SciPost</publisher-name>
</publisher>
<issn>{{ publication.in_issue.in_volume.in_journal.issn }}</issn>
</journal-meta>
<article-meta>
<title-group>
<article-title>{{ publication.title }}</article-title>
</title-group>
<article-id pub-id-type="doi">{{ publication.doi_string }}</article-id>
<volume>{{ publication.in_issue.in_volume.number }}</volume>
<issue>{{ publication.in_issue.number }}</issue>
<pub-date publication-format="epub" date-type="pub" iso-8601-date="{{ publication.publication_date|date:'Y-m-d' }}">
<day>{{ publication.publication_date|date:'d' }}</day>
<month>{{ publication.publication_date|date:'m' }}</month>
<year>{{ publication.publication_date|date:'Y' }}</year>
</pub-date></article-meta>
</front>
</article>
